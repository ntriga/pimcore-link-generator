{
    "name": "ntriga/pimcore-link-generator",
    "description": "This is my package pimcore-link-generator",
    "keywords": [
        "ntriga",
        "pimcore-link-generator"
    ],
    "homepage": "https://github.com/ntriga/pimcore-link-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Bibauw",
            "email": "vincent@ntriga.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Ntriga\\PimcoreLinkGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ntriga\\PimcoreLinkGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
